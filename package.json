        "get-md-title": "^1.0.4",
        "highlight.js": "^9.13.1",
        "js-yaml": "^3.12.0",
        "line-awesome": "icons8/line-awesome",
        "lodash.kebabcase": "^4.1.1",
        "nodegit": "^0.22.2",
        "octicons": "^7.3.0",
        "path-is-inside": "^1.0.2",
        "remarkable": "^1.7.1",
        "remarkable-meta": "^1.0.1",
        "rxjs": "6.2.2",
        "typeface-open-sans": "^0.0.54",
        "uuid": "^3.3.2",
        "zone.js": "^0.8.26"
    },
